{
  "name": "airbnb-clone-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prisma": "npx prisma",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "postinstall": "next build",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset --force",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "^5.12.1",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.0",
    "date-fns": "2.30.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "leaflet": "1.9.4",
    "mongodb": "^6.5.0",
    "next": "^14.2.1",
    "next-auth": "^4.22.1",
    "next-cloudinary": "^6.5.0",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.0",
    "react-hot-toast": "2.4.1",
    "react-icons": "^4.8.0",
    "react-leaflet": "^4.2.1",
    "react-responsive-carousel": "^3.2.23",
    "react-select": "5.7.3",
    "react-spinners": "^0.13.8",
    "sass": "1.63.6",
    "sharp": "^0.33.3",
    "styled-components": "^6.0.0-rc.6",
    "swiper": "9.4.1",
    "swr": "^2.1.1",
    "typescript": "5.0.2",
    "world-countries": "^4.0.0",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.3",
    "@types/react-date-range": "^1.4.4",
    "autoprefixer": "^10.4.14",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.21",
    "prettier": "^3.3.2",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.2.7"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.5.2"
  }
}
